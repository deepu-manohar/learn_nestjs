{
  "moduleDirectories": [
    "node_modules",
    "../src"
  ],
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "../coverage",
  "testEnvironment": "node",
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/../src/$1"
  },
  "rootDir": "./",
  "testRegex": ".e2e-spec.ts$",
  "setupFilesAfterEnv":["<rootDir>/setup.ts"]
}
